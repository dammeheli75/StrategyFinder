#include <Object.mqh>
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
class HttpFile: public CObject
  {
protected:
   string            m_name;
   string            m_path;

public:
                     HttpFile(void);
                     HttpFile(string name,string path);
                    ~HttpFile(void);
   void              Name(string name);
   string            Name(void);
   void              Path(string path);
   string            Path(void);
  };
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
HttpFile::HttpFile(void)
  {
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
HttpFile::HttpFile(string name,string path)
  {
   this.Name(name);
   this.Path(path);
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
HttpFile::~HttpFile(void)
  {
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
void  HttpFile::Name(string name)
  {
   m_name=name;
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
string HttpFile::Name(void)
  {
   if(m_name==NULL) m_name="";

   return(m_name);
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
void HttpFile::Path(string path)
  {
   m_path=path;
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
string HttpFile::Path(void)
  {
   if(m_path==NULL) m_path="";

   return(m_path);
  }
//+------------------------------------------------------------------+
